<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mshr="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >


    <com.mshr.reminder.viewparts.TextInsectionEmptyEditText
        android:id="@+id/text_edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/text_edit_background"
        android:inputType="text"
        android:maxLength="99"
        android:padding="5dp"
        android:textColor="@color/gray"
        android:textSize="25sp"
        mshr:font="@string/m_font_bold" >
    </com.mshr.reminder.viewparts.TextInsectionEmptyEditText>

    <com.mshr.reminder.viewparts.OriginalFontTextView
        android:id="@+id/validator_text_view"
        style="@style/validator_text_view"
        mshr:font="@string/m_font_bold" >
    </com.mshr.reminder.viewparts.OriginalFontTextView>

</FrameLayout>